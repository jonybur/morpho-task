{
  "version": 2,
  "buildCommand": "npm run build && npm run build-storybook",
  "outputDirectory": ".next",
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/storybook",
      "dest": "/storybook-static/index.html"
    },
    {
      "src": "/storybook/(.*)",
      "dest": "/storybook-static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
