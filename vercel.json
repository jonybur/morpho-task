{
  "version": 2,
  "framework": "nextjs",
  "buildCommand": "npm run build && npm run build-storybook",
  "outputDirectory": ".next",
  "public": true,
  "cleanUrls": true,
  "trailingSlash": false,
  "regions": ["iad1"],
  "rewrites": [
    {
      "source": "/storybook",
      "destination": "/storybook-static/index.html"
    },
    {
      "source": "/storybook/:path*",
      "destination": "/storybook-static/:path*"
    }
  ]
}
