{
  "version": 2,
  "buildCommand": "npm run build && npm run build-storybook",
  "installCommand": "npm install",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    },
    {
      "src": "storybook-static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/storybook",
      "dest": "/storybook-static/index.html"
    },
    {
      "src": "/storybook/(.*)",
      "dest": "/storybook-static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
