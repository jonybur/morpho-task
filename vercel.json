{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    },
    {
      "src": "storybook-static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/storybook/(.*)",
      "headers": { "cache-control": "public, max-age=3600" },
      "dest": "/storybook-static/$1"
    },
    {
      "src": "/storybook",
      "dest": "/storybook-static/index.html"
    }
  ]
}
